{
  "name": "Tab Counter",
  "version": "1.1",
  "description": "Displays the current number of open tabs.",
	"background": {
		"service_worker": "background.js"
	},
  "permissions": [
    "tabs"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "incognito": "split",
  "action": {
    "default_icon": {
      "16": "icon128.png",
      "24": "icon128.png",
      "32": "icon128.png"
    },
    "default_title": "Tab Counter"
  },
  "manifest_version": 3
}
